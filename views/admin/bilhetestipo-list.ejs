<% include headerAdmin %>
<% include navAdmin %>


<br>

<div class="page-content-wrap">
	<div class="row">
		<div class="col-md-12">

			<!-- START DEFAULT DATATABLE -->
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 class="panel-title"><a href="/bilhetestipo"><button class="btn btn-danger">Editar preço bilhetes</button></a></h3>
					<ul class="panel-controls">
						<li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
						<li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
						<li><a href="#" class="panel-remove"><span class="fa fa-times"></span></a></li>
					</ul>
				</div>
				<div class="panel-body">
					<table class="table datatable">
						<thead>
						<tr>
							<th>Tipo de Bilhete</th>
							<th>Preço</th>
						</tr>
						</thead>
						<tbody><% for (var u of listatipo) { %>
						<tr>
							<td><%= u.tipoBilhetes %></td>
							<td><a href="/bilhetestipo/<%= u.idTipoBilhete %> "><%= u.preco %> €</a></td>
						</tr><%} %>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>



<% include footerAdmin %>



